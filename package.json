{
  "name": "ng-universal-demo",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/angular/universal-starter.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "lint": "ng lint ng-universal-demo",
    "build:client-and-server-bundles": "ng build --prod && ng run ng-universal-demo:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json && webpack --progress",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "compile:api": "tsc -p api.tsconfig.json",
    "serve:api": "set APP_ENV=development&& node dist/api",
    "serve:api:prod": "set APP_ENV=production&& node dist/api",
    "watch": "webpack --progress --watch",
    "serve:ssr": "set APP_ENV=development&& node dist/server",
    "serve:ssr:prod": "set APP_ENV=production&& node dist/server",
    "preserve": "npx babel node_modules/ngx-bootstrap/** --out-dir node_modules/ngx-bootstrap --presets env",
    "debug": "npm run build:ssr && npm run serve:ssr"
  },
  "pre-commit": [],
  "private": true,
